{
    "parser": "babel-eslint",
    "settings": {
        "react": {
          "version": "detect"
        }
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi-style": ["error", "last"],
        "quotes": [1, "double"]
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    }
}








    
  